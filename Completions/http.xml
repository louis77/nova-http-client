<completions>
	<provider name="http.mimetypes">
		<syntax>http</syntax>
		<selector>http.request.header:has-child(http.string.key:capture(headerkey)) http.value</selector>
		<expression>(\b|-)[a-zA-Z0-9-]*</expression>
		<set>http.${headerkey}</set> 
	</provider>
	
	<provider name="http.headers">
		<syntax>http</syntax>
		<selector>http.request.header http.string.key</selector>
		<expression>\b(.*)</expression>
		<set>http.headers</set>
	</provider>
	
	
	<set name="http.Content-Type">
		<completion string="application/json" />
		<completion string="application/ld+json" />
		<completion string="application/octet-stream" />
		<completion string="application/rtf" />
		<completion string="application/x-sh" />
		<completion string="application/xhtml+xml" />
		<completion string="application/xml" />
		<completion string="text/calendar" />
		<completion string="text/css" />
		<completion string="text/csv" />
		<completion string="text/html" />
		<completion string="text/javascript" />
		<completion string="text/plain" />
		<completion string="text/xml" />
	</set>
	
	<set name="http.Accept">
